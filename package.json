{
  "name": "ripread",
  "description": "Read lots of files as if a saber-tooth tiger is chasing you!",
  "version": "1.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "benchmark": "node tasks/benchmark.js",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage .nyc_output",
    "clean": "npm run clean:dist && npm run clean:coverage",
    "compile": "tsc --skipLibCheck && tstei",
    "compile:watch": "tsc --skipLibCheck --watch",
    "test": "node test/index.js",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "ava": {
    "files": [
      "test/index.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/ripread/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/ripread.git"
  },
  "keywords": [
    "read",
    "files",
    "fast"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "atomically": "^1.7.0",
    "worktank": "^1.1.1"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3",
    "typescript-transform-export-interop": "^1.0.2"
  }
}
